//I don't know anymore

public class ASCIIconversion {


    public static void main(String[] args) {

        for (int i = 1; i < 255; i++) {
            System.out.print(" Decimal: " + i);
            System.out.print(" ASCII: " + (char)i);
            System.out.print(" Base 8: " + Integer.toOctalString(i));

            System.out.println();
        }
    }

}
